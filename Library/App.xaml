<Application x:Class="Library.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:fa="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp" 
             StartupUri="Start/AuthorizationWindow.xaml">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles\Colors.xaml"/>
                <ResourceDictionary Source="Styles\TextBlocks.xaml"/>
                <ResourceDictionary Source="Styles\Separatos.xaml"/>
                <ResourceDictionary Source="Styles\TextBoxs.xaml"/>
                <ResourceDictionary Source="Styles\PasswordBoxs.xaml"/>
                <ResourceDictionary Source="Styles\Buttons.xaml"/>
                <ResourceDictionary Source="Styles\ListViews.xaml"/>
                <ResourceDictionary Source="Styles\IconImages.xaml"/>
                <ResourceDictionary Source="Styles\DocumentViewers.xaml"/>
                <ResourceDictionary Source="Styles\ScrollViewers.xaml"/>
                <ResourceDictionary Source="Styles\ToolTips.xaml"/>
                <ResourceDictionary Source="Styles\ComboBoxes.xaml"/>
                <ResourceDictionary Source="Styles\ToolBars.xaml"/>
                <ResourceDictionary Source="Styles\UserControls.xaml"/>
                <ResourceDictionary Source="Styles\Grids.xaml"/>
                <ResourceDictionary Source="Styles\DatePickers.xaml"/>
                <ResourceDictionary Source="Styles\CheckBoxs.xaml"/>
                <ResourceDictionary Source="Styles\DataGrids.xaml"/>
                <ResourceDictionary Source="Styles\RadioButtons.xaml"/>
                <ResourceDictionary Source="Styles\ToggleButtons.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
